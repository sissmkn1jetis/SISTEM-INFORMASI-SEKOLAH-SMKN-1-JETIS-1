{"ast":null,"code":"import TahunAjarService from \"../../services/thn_ajar.service\";\nimport Swal from \"sweetalert2\";\nexport default {\n  data() {\n    return {\n      loading: false,\n      disabled: false,\n      thn_ajars: {},\n      statusmodal: false,\n      form: {\n        id: \"\",\n        semester_aktif: \"\",\n        thn_ajaran: \"\"\n      }\n    };\n  },\n  computed: {\n    currentUser() {\n      return this.$store.state.auth.user;\n    }\n  },\n  created() {\n    this.getdata();\n  },\n  methods: {\n    showModal() {\n      this.statusmodal = false;\n      this.resetForm();\n      $(\"#modalmuncul\").modal(\"show\");\n    },\n    showModalEdit(tahun) {\n      this.statusmodal = true;\n      $(\"#modalmuncul\").modal(\"show\");\n      this.getTahunAjarById(tahun);\n    },\n    getdata() {\n      this.$Progress.start();\n      TahunAjarService.getTahunAjar().then(response => {\n        this.thn_ajars = response.data;\n        $(function () {\n          $(\"#myTable\").DataTable({\n            responsive: false,\n            lengthChange: true,\n            autoWidth: true,\n            buttons: [\"copy\", \"csv\", \"excel\", \"pdf\", \"print\"]\n          }).buttons().container().appendTo(\"#myTable_wrapper .col-md-6:eq(0)\");\n        });\n      }).catch(e => {\n        console.log(e);\n      });\n      this.$Progress.finish();\n    },\n    getTahunAjarById(id) {\n      TahunAjarService.getTahunAjarById(id).then(response => {\n        this.form = response.data;\n      }).catch(e => {\n        console.log(e);\n      });\n    },\n    simpanData() {\n      this.$Progress.start();\n      this.loading = true;\n      this.disabled = true;\n      TahunAjarService.createTahunAjar(this.form).then(response => {\n        this.from = response.data;\n        console.log(response.data);\n        this.getdata();\n        this.resetForm();\n        $(\"#modalmuncul\").modal(\"hide\");\n        const Toast = Swal.mixin({\n          toast: true,\n          position: \"top-end\",\n          showConfirmButton: false,\n          timer: 3000,\n          timerProgressBar: true,\n          didOpen: toast => {\n            toast.addEventListener(\"mouseenter\", Swal.stopTimer);\n            toast.addEventListener(\"mouseleave\", Swal.resumeTimer);\n          }\n        });\n        Toast.fire({\n          icon: \"success\",\n          title: \"Data Berhasil Tersimpan\"\n        });\n        this.$Progress.finish();\n        this.loading = false;\n        this.disabled = false;\n      }).catch(e => {\n        this.$Progress.fail();\n        this.loading = false;\n        this.disabled = false;\n        console.log(e);\n      });\n    },\n    updateData() {\n      this.$Progress.start();\n      this.loading = true;\n      this.disabled = true;\n      TahunAjarService.updateTahunAjar(this.form.id, this.form).then(response => {\n        this.from = response.data;\n        console.log(response.data);\n        this.getdata();\n        this.resetForm();\n        $(\"#modalmuncul\").modal(\"hide\");\n        const Toast = Swal.mixin({\n          toast: true,\n          position: \"top-end\",\n          showConfirmButton: false,\n          timer: 3000,\n          timerProgressBar: true,\n          didOpen: toast => {\n            toast.addEventListener(\"mouseenter\", Swal.stopTimer);\n            toast.addEventListener(\"mouseleave\", Swal.resumeTimer);\n          }\n        });\n        Toast.fire({\n          icon: \"success\",\n          title: \"Data Berhasil Terupdate\"\n        });\n        this.$Progress.finish();\n        this.loading = false;\n        this.disabled = false;\n      }).catch(e => {\n        this.$Progress.fail();\n        this.loading = false;\n        this.disabled = false;\n        console.log(e);\n      });\n    },\n    deleteData(id) {\n      Swal.fire({\n        title: \"Anda Yakin Ingin Menghapus Data Ini ?\",\n        text: \"Klik Batal Untuk Membatalkan Penghapusan\",\n        icon: \"warning\",\n        showCancelButton: true,\n        confirmButtonColor: \"#3085d6\",\n        cancelButtonColor: \"#d33\",\n        confirmButtonText: \"Hapus\"\n      }).then(result => {\n        if (result.value) {\n          TahunAjarService.deleteTahunAjar(id).then(response => {\n            Swal.fire(\"Terhapus\", \"Data Anda Sudah Terhapus\", \"success\");\n            console.log(response.data);\n            this.getdata();\n          }).catch(() => {\n            Swal.fire(\"Gagal\", \"Data Gagal Terhapus\", \"warning\");\n          });\n        }\n      });\n    },\n    resetForm() {\n      this.form.semester_aktif = \"\";\n      this.form.thn_ajaran = \"\";\n    }\n  }\n};","map":{"version":3,"mappings":"AAiNA,OAAOA,gBAAe,MAAO,iCAAiC;AAC9D,OAAOC,IAAG,MAAO,aAAa;AAC9B,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLC,OAAO,EAAE,KAAK;MACdC,QAAQ,EAAE,KAAK;MACfC,SAAS,EAAE,CAAC,CAAC;MACbC,WAAW,EAAE,KAAK;MAClBC,IAAI,EAAE;QACJC,EAAE,EAAE,EAAE;QACNC,cAAc,EAAE,EAAE;QAClBC,UAAU,EAAE;MACd;IACF,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,WAAW,GAAG;MACZ,OAAO,IAAI,CAACC,MAAM,CAACC,KAAK,CAACC,IAAI,CAACC,IAAI;IACpC;EACF,CAAC;EACDC,OAAO,GAAG;IACR,IAAI,CAACC,OAAO,EAAE;EAChB,CAAC;EACDC,OAAO,EAAE;IACPC,SAAS,GAAG;MACV,IAAI,CAACd,WAAU,GAAI,KAAK;MACxB,IAAI,CAACe,SAAS,EAAE;MAChBC,CAAC,CAAC,cAAc,CAAC,CAACC,KAAK,CAAC,MAAM,CAAC;IACjC,CAAC;IACDC,aAAa,CAACC,KAAK,EAAE;MACnB,IAAI,CAACnB,WAAU,GAAI,IAAI;MACvBgB,CAAC,CAAC,cAAc,CAAC,CAACC,KAAK,CAAC,MAAM,CAAC;MAC/B,IAAI,CAACG,gBAAgB,CAACD,KAAK,CAAC;IAC9B,CAAC;IACDP,OAAO,GAAG;MACR,IAAI,CAACS,SAAS,CAACC,KAAK,EAAE;MACtB5B,gBAAgB,CAAC6B,YAAY,EAAC,CAC3BC,IAAI,CAAEC,QAAQ,IAAK;QAClB,IAAI,CAAC1B,SAAQ,GAAI0B,QAAQ,CAAC7B,IAAI;QAC9BoB,CAAC,CAAC,YAAY;UACZA,CAAC,CAAC,UAAU,EACTU,SAAS,CAAC;YACTC,UAAU,EAAE,KAAK;YACjBC,YAAY,EAAE,IAAI;YAClBC,SAAS,EAAE,IAAI;YACfC,OAAO,EAAE,CAAC,MAAM,EAAE,KAAK,EAAE,OAAO,EAAE,KAAK,EAAE,OAAO;UAClD,CAAC,EACAA,OAAO,EAAC,CACRC,SAAS,EAAC,CACVC,QAAQ,CAAC,kCAAkC,CAAC;QACjD,CAAC,CAAC;MACJ,CAAC,EACAC,KAAK,CAAEC,CAAC,IAAK;QACZC,OAAO,CAACC,GAAG,CAACF,CAAC,CAAC;MAChB,CAAC,CAAC;MACJ,IAA