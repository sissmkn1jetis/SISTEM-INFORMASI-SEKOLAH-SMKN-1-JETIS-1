icon","title","fail","updateData","updateTahunAjar","deleteData","text","showCancelButton","confirmButtonColor","cancelButtonColor","confirmButtonText","result","value","deleteTahunAjar"],"sourceRoot":"","sources":["D:\\Coding\\Backup\\SISTEM-INFORMASI-SEKOLAH-SMKN-1-JETIS-1\\frontend\\src\\components\\DataTahunAjar\\TahunAjarlist.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <section class=\"content-header\">\r\n      <div class=\"container-fluid\">\r\n        <div class=\"row mb-2\">\r\n          <div class=\"col-sm-6\">\r\n            <h1>Data Tahun Ajaran</h1>\r\n          </div>\r\n          <div class=\"col-sm-6\">\r\n            <ol class=\"breadcrumb float-sm-right\">\r\n              <li class=\"breadcrumb-item\">\r\n                <router-link to=\"/dashboard\">Dashboard</router-link>\r\n              </li>\r\n              <li class=\"breadcrumb-item active\">Data Tahun Ajaran</li>\r\n            </ol>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n\r\n    <section class=\"content\">\r\n      <div class=\"container-fluid\">\r\n        <div class=\"row\">\r\n          <div class=\"col-12\">\r\n            <div class=\"card\">\r\n              <div class=\"card-header\">\r\n                <div class=\"card-tools\">\r\n                  <button\r\n                    type=\"button\"\r\n                    class=\"btn btn-success mb-2\"\r\n                    @click=\"showModal\"\r\n                  >\r\n                    Tambah Tahun Ajaran\r\n                  </button>\r\n                </div>\r\n              </div>\r\n              <div class=\"card-body\">\r\n                <div class=\"table-responsive\">\r\n                  <table id=\"myTable\" class=\"table table-bordered table-hover\">\r\n                    <thead>\r\n                      <tr>\r\n                        <th class=\"align-middle text-center\">No</th>\r\n                        <th class=\"align-middle text-center\">Status</th>\r\n                        <th class=\"align-middle text-center\">Tahun</th>\r\n                        <th class=\"align-middle text-center\">Action</th>\r\n                      </tr>\r\n                    </thead>\r\n                    <tbody>\r\n                      <tr v-for=\"(tahun, index) in thn_ajars\" :key=\"index.id\">\r\n                        <td class=\"align-middle text-center\">\r\n                          {{ index + 1 }}\r\n                        </td>\r\n                        <td\r\n                          class=\"align-middle text-center\"\r\n                          v-if=\"tahun && tahun.semester_aktif === 'aktif'\"\r\n                        >\r\n                          <span class=\"badge bg-success\">{{\r\n                            tahun.semester_aktif\r\n                          }}</span>\r\n                        </td>\r\n\r\n                        <td\r\n                          class=\"align-middle text-center\"\r\n                          v-if=\"tahun && tahun.semester_aktif === 'tidak-aktif'\"\r\n                        >\r\n                          <span class=\"badge bg-danger\">{{\r\n                            tahun.semester_aktif\r\n                          }}</span>\r\n                        </td>\r\n                        <td class=\"align-middle text-center\">\r\n                          {{ tahun.thn_ajaran }}\r\n                        </td>\r\n                        <td class=\"align-middle text-center\">\r\n                          <a\r\n                            href=\"#\"\r\n                            class=\"badge bg-info mr-2\"\r\n                            @click=\"showModalEdit(tahun.id)\"\r\n                            ><i class=\"fas fa-edit\"></i\r\n                          ></a>\r\n                          <a\r\n                            href=\"#\"\r\n                            class=\"badge bg-danger\"\r\n                            @click=\"deleteData(tahun.id)\"\r\n                            ><i class=\"fas fa-trash-alt\"></i\r\n                          ></a>\r\n                        </td>\r\n                      </tr>\r\n                    </tbody>\r\n                  </table>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div\r\n        class=\"modal fade\"\r\n        id=\"modalmuncul\"\r\n        tabindex=\"-1\"\r\n        role=\"dialog\"\r\n        aria-labelledby=\"modalmuncul1\"\r\n        aria-hidden=\"true\"\r\n      >\r\n        <div class=\"modal-dialog modal-lg\">\r\n          <div class=\"modal-content\">\r\n            <div class=\"modal-header\">\r\n              <h5\r\n                class=\"modal-title\"\r\n                id=\"exampleModalLongTitle\"\r\n                v-show=\"!statusmodal\"\r\n              >\r\n                Tambah Tahun Ajaran\r\n              </h5>\r\n              <h5\r\n                class=\"modal-title\"\r\n                id=\"exampleModalLongTitle\"\r\n                v-show=\"statusmodal\"\r\n              >\r\n                Ubah Tahun Ajaran\r\n              </h5>\r\n              <button\r\n                type=\"button\"\r\n                class=\"close\"\r\n                data-dismiss=\"modal\"\r\n                aria-label=\"Close\"\r\n              >\r\n                <span aria-hidden=\"true\" @click=\"resetForm()\">&times;</span>\r\n              </button>\r\n            </div>\r\n            <div class=\"modal-body\">\r\n              <div class=\"card card-default\">\r\n                <!-- /.card-header -->\r\n                <!-- form start -->\r\n                <form\r\n                  @submit.prevent=\"statusmodal ? updateData() : simpanData()\"\r\n                >\r\n                  <div class=\"card-body\">\r\n                    <div class=\"form-group row\">\r\n                      <label\r\n                        class=\"col-sm-3 col-form-label\"\r\n                        for=\"semester_aktif\"\r\n                        >Status:</label\r\n                      >\r\n                      <div class=\"col-sm-9\">\r\n                        <select\r\n                          class=\"form-control select2\"\r\n                          v-model=\"form.semester_aktif\"\r\n                          required\r\n                        >\r\n                          <option disabled value=\"\">-- Pilih --</option>\r\n                          <option value=\"aktif\">Aktif</option>\r\n                          <option value=\"tidak-aktif\">Tidak Aktif</option>\r\n                        </select>\r\n                      </div>\r\n                    </div>\r\n                    <div class=\"form-group row\">\r\n                      <label class=\"col-sm-3 col-form-label\" for=\"thn_ajaran\"\r\n                        >Tahun Ajaran:</label\r\n                      >\r\n                      <div class=\"col-sm-9\">\r\n                        <input\r\n                          type=\"text\"\r\n                          class=\"form-control\"\r\n                          id=\"thn_ajaran\"\r\n                          placeholder=\"Tahun Ajaran\"\r\n                          v-model=\"form.thn_ajaran\"\r\n                          required\r\n                        />\r\n                      </div>\r\n                    </div>\r\n                  </div>\r\n                  <div class=\"modal-footer\">\r\n                    <button\r\n                      type=\"button\"\r\n                      class=\"btn btn-secondary\"\r\n                      data-dismiss=\"modal\"\r\n                      @click=\"resetForm()\"\r\n                    >\r\n                      Close\r\n                    </button>\r\n                    <button\r\n                      type=\"submit\"\r\n                      class=\"btn btn-primary\"\r\n                      :disabled=\"disabled\"\r\n                      v-show=\"!statusmodal\"\r\n                    >\r\n                      <i v-show=\"loading\" class=\"fa fa-spinner fa-spin\"></i>\r\n                      Simpan\r\n                    </button>\r\n                    <button\r\n                      type=\"submit\"\r\n                      class=\"btn btn-success\"\r\n                      :disabled=\"disabled\"\r\n                      v-show=\"statusmodal\"\r\n                    >\r\n                      <i v-show=\"loading\" class=\"fa fa-spinner fa-spin\"></i>\r\n                      Ubah\r\n                    </button>\r\n                  </div>\r\n                </form>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n  </div>\r\n</template>\r\n<script>\r\nimport TahunAjarService from \"../../services/thn_ajar.service\";\r\nimport Swal from \"sweetalert2\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      loading: false,\r\n      disabled: false,\r\n      thn_ajars: {},\r\n      statusmodal: false,\r\n      form: {\r\n        id: \"\",\r\n        semester_aktif: \"\",\r\n        thn_ajaran: \"\",\r\n      },\r\n    };\r\n  },\r\n  computed: {\r\n    currentUser() {\r\n      return this.$store.state.auth.user;\r\n    },\r\n  },\r\n  created() {\r\n    this.getdata();\r\n  },\r\n  methods: {\r\n    showModal() {\r\n      this.statusmodal = false;\r\n      this.resetForm();\r\n      $(\"#modalmuncul\").modal(\"show\");\r\n    },\r\n    showModalEdit(tahun) {\r\n      this.statusmodal = true;\r\n      $(\"#modalmuncul\").modal(\"show\");\r\n      this.getTahunAjarById(tahun);\r\n    },\r\n    getdata() {\r\n      this.$Progress.start();\r\n      TahunAjarService.getTahunAjar()\r\n        .then((response) => {\r\n          this.thn_ajars = response.data;\r\n          $(function () {\r\n            $(\"#myTable\")\r\n              .DataTable({\r\n                responsive: false,\r\n                lengthChange: true,\r\n                autoWidth: true,\r\n                buttons: [\"copy\", \"csv\", \"excel\", \"pdf\", \"print\"],\r\n              })\r\n              .buttons()\r\n              .container()\r\n              .appendTo(\"#myTable_wrapper .col-md-6:eq(0)\");\r\n          });\r\n        })\r\n        .catch((e) => {\r\n          console.log(e);\r\n        });\r\n      this.$Progress.finish();\r\n    },\r\n    getTahunAjarById(id) {\r\n      TahunAjarService.getTahunAjarById(id)\r\n        .then((response) => {\r\n          this.form = response.data;\r\n        })\r\n        .catch((e) => {\r\n          console.log(e);\r\n        });\r\n    },\r\n    simpanData() {\r\n      this.$Progress.start();\r\n      this.loading = true;\r\n      this.disabled = true;\r\n      TahunAjarService.createTahunAjar(this.form)\r\n        .then((response) => {\r\n          this.from = response.data;\r\n          console.log(response.data);\r\n          this.getdata();\r\n          this.resetForm();\r\n          $(\"#modalmuncul\").modal(\"hide\");\r\n          const Toast = Swal.mixin({\r\n            toast: true,\r\n            position: \"top-end\",\r\n            showConfirmButton: false,\r\n            timer: 3000,\r\n            timerProgressBar: true,\r\n            didOpen: (toast) => {\r\n              toast.addEventListener(\"mouseenter\", Swal.stopTimer);\r\n              toast.addEventListener(\"mouseleave\", Swal.resumeTimer);\r\n            },\r\n          });\r\n\r\n          Toast.fire({\r\n            icon: \"success\",\r\n            title: \"Data Berhasil Tersimpan\",\r\n          });\r\n          this.$Progress.finish();\r\n          this.loading = false;\r\n          this.disabled = false;\r\n        })\r\n        .catch((e) => {\r\n          this.$Progress.fail();\r\n          this.loading = false;\r\n          this.disabled = false;\r\n          console.log(e);\r\n        });\r\n    },\r\n    updateData() {\r\n      this.$Progress.start();\r\n      this.loading = true;\r\n      this.disabled = true;\r\n      TahunAjarService.updateTahunAjar(this.form.id, this.form)\r\n        .then((response) => {\r\n          this.from = response.data;\r\n          console.log(response.data);\r\n          this.getdata();\r\n          this.resetForm();\r\n          $(\"#modalmuncul\").modal(\"hide\");\r\n          const Toast = Swal.mixin({\r\n            toast: true,\r\n            position: \"top-end\",\r\n            showConfirmButton: false,\r\n            timer: 3000,\r\n            timerProgressBar: true,\r\n            didOpen: (toast) => {\r\n              toast.addEventListener(\"mouseenter\", Swal.stopTimer);\r\n              toast.addEventListener(\"mouseleave\", Swal.resumeTimer);\r\n            },\r\n          });\r\n\r\n          Toast.fire({\r\n            icon: \"success\",\r\n            title: \"Data Berhasil Terupdate\",\r\n          });\r\n          this.$Progress.finish();\r\n          this.loading = false;\r\n          this.disabled = false;\r\n        })\r\n        .catch((e) => {\r\n          this.$Progress.fail();\r\n          this.loading = false;\r\n          this.disabled = false;\r\n          console.log(e);\r\n        });\r\n    },\r\n\r\n    deleteData(id) {\r\n      Swal.fire({\r\n        title: \"Anda Yakin Ingin Menghapus Data Ini ?\",\r\n        text: \"Klik Batal Untuk Membatalkan Penghapusan\",\r\n        icon: \"warning\",\r\n        showCancelButton: true,\r\n        confirmButtonColor: \"#3085d6\",\r\n        cancelButtonColor: \"#d33\",\r\n        confirmButtonText: \"Hapus\",\r\n      }).then((result) => {\r\n        if (result.value) {\r\n          TahunAjarService.deleteTahunAjar(id)\r\n            .then((response) => {\r\n              Swal.fire(\"Terhapus\", \"Data Anda Sudah Terhapus\", \"success\");\r\n              console.log(response.data);\r\n              this.getdata();\r\n            })\r\n            .catch(() => {\r\n              Swal.fire(\"Gagal\", \"Data Gagal Terhapus\", \"warning\");\r\n            });\r\n        }\r\n      });\r\n    },\r\n\r\n    resetForm() {\r\n      this.form.semester_aktif = \"\";\r\n      this.form.thn_ajaran = \"\";\r\n    },\r\n  },\r\n};\r\n</script>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}